{
  "expo": {
    "name": "Pokemon",
    "slug": "pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Pokemon_Home_Logo.png",
    "scheme": "Pokemon",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Pokemon_Home_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.opencompass.pokedex",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Pokemon_Home_Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.opencompass.pokedex"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Pokemon_Home_Logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7587f1d0-9c15-4b0c-966c-2941c11fda42"
      }
    },
    "owner": "opencompass"
  }
}
