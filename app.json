{
  "expo": {
    "name": "Pokemon",
    "slug": "pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Pokemon_Home_Logo.png",
    "scheme": "Pokemon",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Pokemon_Home_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.opencompass.pokedex",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "L'application a besoin d'accéder à votre caméra pour prendre des photos de Pokémon.",
        "NSPhotoLibraryUsageDescription": "L'application a besoin d'accéder à vos photos pour sélectionner et sauvegarder des images de Pokémon.",
        "NSPhotoLibraryAddUsageDescription": "L'application a besoin d'accéder à vos photos pour sauvegarder des images de Pokémon."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Pokemon_Home_Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.opencompass.pokedex",
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Pokemon_Home_Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "L'application a besoin d'accéder à votre caméra pour prendre des photos de Pokémon."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application a besoin d'accéder à vos photos pour sélectionner des images de Pokémon."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7587f1d0-9c15-4b0c-966c-2941c11fda42"
      }
    },
    "owner": "opencompass",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7587f1d0-9c15-4b0c-966c-2941c11fda42"
    }
  }
}